        ============================================ -->
    <script src="~/Scripts/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Scripts/js/scrollbar/mCustomScrollbar-active.js"></script>
    <!-- metisMenu JS
        ============================================ -->
    <script src="~/Scripts/js/metisMenu/metisMenu.min.js"></script>
    <script src="~/Scripts/js/metisMenu/metisMenu-active.js"></script>
    <!-- morrisjs JS
        ============================================ -->
    <script src="~/Scripts/js/morrisjs/raphael-min.js"></script>
    <script src="~/Scripts/js/morrisjs/morris.js"></script>
    <script src="~/Scripts/js/morrisjs/morris-active.js"></script>
    <!-- morrisjs JS
        ============================================ -->
    <script src="~/Scripts/js/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Scripts/js/sparkline/jquery.charts-sparkline.js"></script>
    <script src="~/Scripts/js/sparkline/sparkline-active.js"></script>
    <!-- calendar JS
        ============================================ -->
    <script src="~/Scripts/js/calendar/moment.min.js"></script>
    <script src="~/Scripts/js/calendar/fullcalendar.min.js"></script>
    <script src="~/Scripts/js/calendar/fullcalendar-active.js"></script>
    <!-- data table JS
        ============================================ -->
    <script src="~/Scripts/js/data-table/bootstrap-table.js"></script>
    <script src="~/Scripts/js/data-table/tableExport.js"></script>
    <script src="~/Scripts/js/data-table/data-table-active.js"></script>
    <script src="~/Scripts/js/data-table/bootstrap-table-editable.js"></script>
    <script src="~/Scripts/js/data-table/bootstrap-editable.js"></script>
    <script src="~/Scripts/js/data-table/bootstrap-table-resizable.js"></script>
    <script src="~/Scripts/js/data-table/colResizable-1.5.source.js"></script>
