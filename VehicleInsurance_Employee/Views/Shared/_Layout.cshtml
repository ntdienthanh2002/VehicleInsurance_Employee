    <!--  editable JS
        ============================================ -->
    <script src="~/Scripts/js/editable/jquery.mockjax.js"></script>
    <script src="~/Scripts/js/editable/mock-active.js"></script>
    <script src="~/Scripts/js/editable/select2.js"></script>
    <script src="~/Scripts/js/editable/moment.min.js"></script>
    <script src="~/Scripts/js/editable/bootstrap-datetimepicker.js"></script>
    <script src="~/Scripts/js/editable/bootstrap-editable.js"></script>
    <script src="~/Scripts/js/editable/xediable-active.js"></script>
    <!-- Chart JS
        ============================================ -->
    <script src="~/Scripts/js/chart/jquery.peity.min.js"></script>
    <script src="~/Scripts/js/peity/peity-active.js"></script>
    <!-- tab JS
        ============================================ -->
    <script src="~/Scripts/js/tab.js"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="~/Scripts/js/plugins.js"></script>
    <!-- main JS
        ============================================ -->
    <script src="~/Scripts/js/main.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
